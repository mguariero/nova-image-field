{
    "name": "mguariero/nova-advanced-image-field",
    "description": "An advanced image field for Nova with crop and resize.",
    "keywords": [
        "laravel",
        "nova",
        "image",
        "crop",
        "resize",
        "image crop",
        "image resize",
        "advanced",
        "advanced image",
        "advanced image field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "mGuariero\\NovaImageField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mGuariero\\NovaImageField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "ext-exif": "Required to orientate images automatically."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
